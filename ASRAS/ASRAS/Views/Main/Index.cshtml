@{

    ViewBag.Title = "View";
    //var Uname = ViewBag.Uname;
    var Uname2 = Session["UserName"];
 <html>
 <head>
     <meta charset="utf-8" />
     <meta name="viewport" content="width=device-width, initial-scale=1.0">
     <title>@ViewBag.Title - ASRAS</title>
     <link href="@Url.Content("~/Content/style.css")" rel="stylesheet" type="text/css" />
     @Styles.Render("~/Content/css")
     @Scripts.Render("~/bundles/modernizr")
     
 </head>
<body>
    <script type="text/javascript">
       
        function GetProposals()
        {
            console.log("entered Function");
            //var u = @Uname2;
            document.getElementById('Proposals').innerHTML = " \@Html.Action("GetProposals",new { Uname = Uname2 })\ ";
        }
    </script>
    <h1> Welcome @Uname2</h1>
    <div class="container">
        <div.one class="jumbotron" style="width:300px;float: left;">
            <img src="~/images/url.gif" align="left"/>
        </div.one>
        <div.two style="float:right;width:700px">
            <ul class="nav nav-tabs">
                <li role="presentation" class="active"><a href="#create" data-toggle="tab">Create Proposal</a></li>
                <li role="presentation"><a href="#view" data-toggle="tab" onclick="GetProposals()">View Proposals</a></li>
            </ul>
            <div class="tab-content">
                <div id="create" class="tab-pane fade in active" style="align-content:center;">
                    <div class="jumbotron">
                        <a href="../../ChangeInCurriculum/Index" class="list-group-item">Change in Curriculum </a>
                        <a href="../../ChangeInCourseStructure/Index" class="list-group-item">Change in Course structure </a>
                        <a href="../../ChangeInRegulations/Index" class="list-group-item">Change in Regulation </a>
                    </div>
                </div>
                <div id="view" class="tab-pane fade">
                    <div class="jumbotron">
                        <div id="Proposals">
                            
                        </div>
                    </div>
                </div>
            </div>
        </div.two>
    </div>
 </body>
</html>
}