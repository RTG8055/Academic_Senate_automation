@{

    ViewBag.Title = "View";
    var Uname = ViewBag.Uname;
 <html>
 <head>
     <meta charset="utf-8" />
     <meta name="viewport" content="width=device-width, initial-scale=1.0">
     <title>@ViewBag.Title - ASRAS</title>
     <link href="@Url.Content("~/Content/style.css")" rel="stylesheet" type="text/css" />
     @Styles.Render("~/Content/css")
     @Scripts.Render("~/bundles/modernizr")
     
 </head>

<body>
    <div class="container" style="margin-top:70px">
        <div.one class="jumbotron" style="width:300px;float: left; text-align:center;">
            <div><img src="~/images/url.gif"></div>
            <div></div>
        </div.one>
        <div.two style="float:right;width:700px">
            <ul class="nav nav-tabs">
                <li role="presentation" class="active"><a href="#create" data-toggle="tab">Create Proposal</a></li>
                <li role="presentation"><a href="#view" data-toggle="tab">View Proposals</a></li>
            </ul>
            <div class="tab-content">
                <div id="create" class="tab-pane fade in active" style="align-content:center;">
                    <div class="jumbotron">
                        <a href="../../ChangeInCurriculum/Index" class="list-group-item">Change in Curriculum </a>
                        <a href="../../ChangeInCourseStructure/Index" class="list-group-item">Change in Course structure </a>
                        <a href="../../ChangeInRegulations/Index" class="list-group-item">Change in Regulation </a>
                    </div>
                </div>
                <div id="view" class="tab-pane fade">
                    <div class="jumbotron" >
                        <a href="../../components/#navbar" class="list-group-item">Proposal 1</a>
                        <a href="../../components/#navbar" class="list-group-item">Proposal 2</a>
                        <a href="../../components/#navbar" class="list-group-item">Proposal 3</a>
                    </div>
                </div>
